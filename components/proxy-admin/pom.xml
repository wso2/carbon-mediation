<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <parent>
        <groupId>org.wso2.carbon</groupId>
        <artifactId>carbon-mediation-components</artifactId>
        <version>${wso2carbon.version}</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <artifactId>proxy-admin</artifactId>
    <packaging>pom</packaging>
    <name>WSO2 Carbon - Proxy Services Admin Aggregator Module</name>
    <url>http://wso2.org</url>

    <modules>
        <module>org.wso2.carbon.proxyadmin</module>
        <!--module>org.wso2.carbon.proxyadmin.ui</module>
        <module>org.wso2.carbon.proxyadmin.common</module-->
    </modules>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.core</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.mediation.initializer</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.synapse</groupId>
                <artifactId>synapse-core</artifactId>
                <version>${synapse.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.service.mgt</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.rampart.wso2</groupId>
                <artifactId>rampart-core</artifactId>
                <version>${rampart.wso2.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.axis2.wso2</groupId>
                <artifactId>axis2</artifactId>
                <version>${axis2.wso2.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.ui</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.utils</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.registry.core</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
	    <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.logging</artifactId>
                <version>${wso2carbon.version.proxy-admin}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <properties>
        <wso2carbon.version.proxy-admin>${wso2carbon.version}</wso2carbon.version.proxy-admin>
        <axis2.wso2.version.proxy-admin>${axis2.wso2.version}</axis2.wso2.version.proxy-admin>
        <axis2.osgi.version.range.proxy-admin>${axis2.osgi.version.range}</axis2.osgi.version.range.proxy-admin>
        <axiom.osgi.version.range.proxy-admin>${axiom.osgi.version.range}</axiom.osgi.version.range.proxy-admin>
        <rampart.wso2.version.proxy-admin>${rampart.wso2.version}</rampart.wso2.version.proxy-admin>
        <synapse.version.proxy-admin>${synapse.version}</synapse.version.proxy-admin>
    </properties>

</project>

