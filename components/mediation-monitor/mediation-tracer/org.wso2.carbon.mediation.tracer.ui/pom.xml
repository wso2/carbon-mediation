<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
   <parent>
      <groupId>org.wso2.carbon.mediation</groupId>
      <artifactId>mediation-tracer</artifactId>
      <version>4.3.0</version>
   </parent>
   <modelVersion>4.0.0</modelVersion>
   <groupId>org.wso2.carbon.mediation</groupId>
   <artifactId>org.wso2.carbon.mediation.tracer.ui</artifactId>
   <packaging>bundle</packaging>
   <name>WSO2 Carbon - Mediation Tracer UI</name>
   <version>4.3.0</version>
   <url>http://wso2.org</url>
   <build>
      <plugins>
         <plugin>
            <groupId>org.apache.felix</groupId>
            <artifactId>maven-bundle-plugin</artifactId>
            <extensions>true</extensions>
            <configuration>
               <instructions>
                  <Bundle-SymbolicName>${project.artifactId}</Bundle-SymbolicName>
                  <Bundle-Name>${project.artifactId}</Bundle-Name>
                  <Carbon-Component>UIBundle</Carbon-Component>
                  <Export-Package>org.wso2.carbon.mediation.tracer.ui.*</Export-Package>
                  <Import-Package>org.wso2.carbon.mediation.tracer.stub.*; version="${carbon.platform.package.export.version}",
                            org.wso2.carbon.mediation.tracer.stub.client.*; version="${carbon.platform.package.export.version}"</Import-Package>
                  <DynamicImport-Package>*</DynamicImport-Package>
               </instructions>
            </configuration>
         </plugin>
      </plugins>
   </build>
   <dependencies>
      <dependency>
         <groupId>org.wso2.carbon</groupId>
         <artifactId>org.wso2.carbon.ui</artifactId>
      </dependency>
      <dependency>
         <groupId>org.wso2.carbon</groupId>
         <artifactId>org.wso2.carbon.logging</artifactId>
      </dependency>
      <dependency>
         <groupId>org.wso2.carbon.mediation</groupId>
         <artifactId>org.wso2.carbon.mediation.tracer.stub</artifactId>
      </dependency>
   </dependencies>
</project>
