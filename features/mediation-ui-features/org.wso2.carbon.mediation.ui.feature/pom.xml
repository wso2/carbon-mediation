    <build>
        <plugins>
            <plugin>
                <groupId>org.wso2.maven</groupId>
                <artifactId>carbon-p2-plugin</artifactId>
                <version>${carbon.p2.plugin.version}</version>
                <executions>
                    <execution>
                        <id>4-p2-feature-generation</id>
                        <phase>package</phase>
                        <goals>
                            <goal>p2-feature-gen</goal>
                        </goals>
                        <configuration>
                            <id>org.wso2.carbon.mediation.ui</id>
                            <propertiesFile>../../etc/feature.properties</propertiesFile>
                            <adviceFile>
                                <properties>
                                    <propertyDef>org.wso2.carbon.p2.category.type:server</propertyDef>
                                    <propertyDef>org.eclipse.equinox.p2.type.group:false</propertyDef>
                                </properties>
                            </adviceFile>
                            <bundles>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.endpoint.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.endpoint.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediation.artifactuploader.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediation.artifactuploader.stub</bundleDef>
