                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.message.store.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.priority.executors.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.priority.executors.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.proxyadmin.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.proxyadmin.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.proxyadmin.common</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.rest.api.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.rest.api.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.sequences.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.sequences.common</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediator.service</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.sequences.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.task.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.task.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediation.library.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediation.library.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediation.security.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.mediation.security.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.localentry.ui</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.localentry.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.inbound.stub</bundleDef>
                                <bundleDef>org.wso2.carbon.mediation:org.wso2.carbon.inbound.ui</bundleDef>
                            </bundles>
                            <importFeatures>
                                <importFeatureDef>org.wso2.carbon.core.server:${carbon.kernel.version}</importFeatureDef>
                                <importFeatureDef>org.wso2.carbon.core.ui:${carbon.kernel.version}</importFeatureDef>
                                <importFeatureDef>org.apache.synapse.wso2:${carbon.mediation.version}</importFeatureDef>
                                <importFeatureDef>org.wso2.carbon.mediation.initializer.server:${carbon.mediation.version}
                                </importFeatureDef>
                                <importFeatureDef>org.wso2.carbon.mediation.admin:${carbon.mediation.version}
                                </importFeatureDef>
                            </importFeatures>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
